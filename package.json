{
  "name": "snyk-security-example",
  "version": "0.1.0",
  "description": "Node.js project demonstrating Snyk vulnerability scanning integration",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx --watch src/index.ts",
    "test": "node --test dist/test/*.js",
    "test:watch": "node --test --watch dist/test/",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "build": "tsc && tsc -p test/tsconfig.json",
    "clean": "rimraf dist coverage",
    "security:test": "snyk test"
  },
  "keywords": [
    "nodejs",
    "snyk",
    "security",
    "vulnerability-scanning",
    "ci-cd",
    "github-actions"
  ],
  "author": {
    "name": "Dzianis Buyakou",
    "email": "dzianis.buyakou@gmail.com"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.10.0",
  "dependencies": {
    "express": "^4.18.0",
    "lodash": "4.17.20"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/express": "^4.17.0",
    "@types/lodash": "^4.17.0",
    "@eslint/js": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^6.0.0",
    "typescript": "^5.0.0",
    "tsx": "^4.0.0",
    "snyk": "^1.1200.0"
  }
}
